{
  "name": "@propflow/property-service",
  "version": "1.0.0",
  "description": "PropFlow property management microservice",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "npx typeorm migration:run",
    "migrate:generate": "npx typeorm migration:generate",
    "migrate:revert": "npx typeorm migration:revert",
    "seed": "ts-node src/database/seeders/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "apollo-server-express": "^3.12.1",
    "@apollo/subgraph": "^2.5.1",
    "graphql": "^16.8.1",
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "redis": "^4.6.8",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.5",
    "aws-sdk": "^2.1462.0",
    "zod": "^3.22.2",
    "winston": "^3.10.0",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "elasticsearch": "^16.7.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/multer": "^1.4.7",
    "@types/elasticsearch": "^5.0.40",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.6.2",
    "@types/jest": "^29.5.4",
    "ts-jest": "^29.1.1",
    "eslint": "^8.47.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0"
  }
}